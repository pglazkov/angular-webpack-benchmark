{
  "name": "angular-webpack-benchmark",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "generate": "tsc generate.ts && node generate.js && rimraf generate.js",
    "build": "yarn run webpack",
    "build:happy": "yarn run webpack --env.HAPPYPACK",
    "build:prod": "yarn run ngc && yarn run webpack --env.PROD --env.AOT && yarn run clean:ngc",
    "build:prod:happy": "yarn run ngc && yarn run webpack --env.PROD --env.AOT --env.HAPPYPACK && yarn run clean:ngc",
    "clean:ngc": "rimraf aot_temp",
    "clean": "rimraf dist aot_temp",
    "webpack": "node --max_old_space_size=16384 node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "node --max_old_space_size=16384 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "ngc": "ngc -p ./tsconfig.prod.json",
    "start": "yarn run webpack-dev-server --config webpack.config.js --watch --content-base dist",
    "rimraf": "rimraf"
  },
  "dependencies": {
    "@angular/common": "4.4.6",
    "@angular/compiler": "4.4.6",
    "@angular/compiler-cli": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@angular/router": "4.4.6",
    "core-js": "2.5.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.5.2",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.0.33",
    "@types/node": "8.0.47",
    "angular-router-loader": "^0.8.0",
    "angular2-template-loader": "^0.6.2",
    "clean-webpack-plugin": "0.1.17",
    "css-loader": "0.28.7",
    "file-loader": "1.1.5",
    "fork-ts-checker-webpack-plugin": "0.2.8",
    "happypack": "4.0.0",
    "html-webpack-plugin": "2.30.1",
    "json-loader": "0.5.7",
    "node-sass": "4.5.3",
    "optimize-js-plugin": "0.0.4",
    "raw-loader": "0.5.1",
    "rimraf": "^2.6.1",
    "rmfr": "^1.0.3",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.3",
    "style-loader": "0.19.0",
    "to-string-loader": "1.1.5",
    "ts-loader": "3.1.1",
    "tslib": "1.8.0",
    "typescript": "2.6.1",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pglazkov/angular-webpack-benchmark.git"
  },
  "engines": {
    "node": ">= 6.9.4",
    "npm": ">= 3"
  }
}
